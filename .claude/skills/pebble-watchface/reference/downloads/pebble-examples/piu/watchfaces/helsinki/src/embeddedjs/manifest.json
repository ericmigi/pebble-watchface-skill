{
	"config": {
		"icon": "./icon.png",
		"watchface": true
	},
	"modules": {
		"*": "./main.js"
	},
	"platforms": {
		"*/flint": {
			"modules": {
				"*": "./flint/layout.js"
			},
			"resources": {
				"*-alpha": "./flint/digits"
			}
		},
		"*/emery": {
			"modules": {
				"*": "./emery/layout.js"
			},
			"resources": {
				"*-alpha": "./emery/digits"
			}
		},
		"...": {
			"error": "Platform not supported"
		}
	}
}
